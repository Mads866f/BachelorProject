@using DTO.Models
<MudItem xs="4">
    <MudStack Style ="max-height: 500px; overflow-y: auto">
        @foreach (var voter in Voters)
        {
            <SingleVoterView
            Voter="voter"/>
        }
    </MudStack>
</MudItem>
@code {
    [Parameter] public required List<Voter> Voters { get; set; }
}